<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundImageView">
        <attr name="circleWith" format="dimension"/><!--圆环宽度-->
        <attr name="circleOutSideColor" format="color"/><!--外半圆颜色-->
        <attr name="circleInSideColor" format="color"/><!--内半圆颜色-->
    </declare-styleable>
    <declare-styleable name="TabRadioButton">
        <attr name="drawableWidth" format="dimension"/>
        <attr name="drawableHeight" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="SlidingMenu">
        <attr name="rightPadding" format="dimension"/><!--菜单打开时距右边的距离-->
        <attr name="showAnim" format="boolean"/><!--是否显示动画-->
    </declare-styleable>
</resources>